<openerp>
<data noupdate="1">
<!-- MRO Operation -->
<record id="sequence_mro_property" model="ir.sequence">
    <field name="name">MRO Property</field>
    <field name="padding">6</field>
    <field name="prefix">MRO-PRO/%(y)s/</field>
    <field eval="1" name="number_increment"/>
</record>

<!-- MRO Operation Maintenance-->
<record id="sequence_mro_maintenance_property" model="ir.sequence">
    <field name="name">MRO Maintenance Property</field>
    <field name="padding">6</field>
    <field name="prefix">MTN-PRO/%(y)s/</field>
    <field eval="1" name="number_increment"/>
</record>
</data>
</openerp>
